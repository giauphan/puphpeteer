{
    "name": "giauphan/puphpeteer",
    "description": "A Puppeteer bridge for PHP, supporting the entire API. Fix trouble when install ",
    "keywords": [
        "php",
        "puppeteer",
        "headless-chrome",
        "testing",
        "web",
        "developer-tools",
        "automation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Giau Phan",
            "email": "giauphan012@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "giauphan/rialto": "*",
        "psr/log": "*",
        "vierbergenlars/php-semver": "^3.0.2"
    },
    "require-dev": {
        "monolog/monolog": "^2.0",
        "phpunit/phpunit": "^9.0",
        "symfony/process": "^4.0|^5.0",
        "symfony/console": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nesk\\Puphpeteer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nesk\\Puphpeteer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "npm install",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
